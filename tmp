
<:: symval

symval_quote :- {t} @{\\t } @'COMBINE '.
symval_quote :- {n} @{\\n } @'COMBINE '.
symval_quote :- \any @'APPEND ' &''.

symval_special :- {'}.
symval_special :- {\\}.

symval_str :- !symval_special symval_str.
symval_str :- .

symval_run :- {'}.
symval_run :- {\\} symval_quote symval_run.
symval_run :- @'PUSH ' @{'' } symval_str @'APPEND \'' &'' @{' }symval_run.



symval :- {'} @'START[' symval_run @']END'.
::>

<:[symval]'hello \'world\'':>

